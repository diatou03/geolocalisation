{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "berkayk/onesignal-laravel": "^2.4",
        "blade-ui-kit/blade-heroicons": "^2.6",
        "blade-ui-kit/blade-icons": "^1.8",
        "blockpoint/laravel-favicon-generator": "^1.1",
        "cmfcmf/openweathermap-php-api": "^3.3",
        "composer/ca-bundle": "^1.5",
        "dnsimmons/openweather": "^1.0",
        "geoip2/geoip2": "3.2",
        "grigorygerasimov/laravel-weather": "*",
        "guzzlehttp/guzzle": "^7.9",
        "kevinrob/guzzle-cache-middleware": "^6.0",
        "kreait/firebase-php": "^7.22",
        "kreait/laravel-firebase": "^6.1",
        "laravel-notification-channels/fcm": "^5.1",
        "laravel-notification-channels/twilio": "^4.1",
        "laravel-notification-channels/webpush": "^10.2",
        "laravel/framework": "^12.0",
        "laravel/sanctum": "^4.1",
        "laravel/tinker": "^2.10.1",
        "larswiegers/laravel-maps": "^0.19.0",
        "owenvoke/blade-fontawesome": "^2.9",
        "php-mqtt/client": "^2.2",
        "php-mqtt/laravel-client": "^1.6",
        "predis/predis": "^3.2",
        "rakibdevs/openweather-laravel-api": "^1.9",
        "spatie/laravel-navigation": "^1.3",
        "spatie/menu": "^3.2",
        "toin0u/geocoder-laravel": "^5.0",
        "torann/geoip": "^3.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/breeze": "^2.3",
        "laravel/pail": "^1.2.2",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.41",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "phpunit/phpunit": "^11.5.3",
        "timwassenburg/laravel-service-generator": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite"
        ],
        "test": [
            "@php artisan config:clear --ansi",
            "@php artisan test"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
    
}
